{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "strict": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "rootDirs": ["./configs/enzyme","src"],
    "outDir": "dist",
    "skipLibCheck": true,
    "isolatedModules": true,
    "baseUrl": "src"
  },
  "include": [
    "src"
  ]
}
